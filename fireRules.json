{
  "rules": {
    ".read": "true",
    ".write": "true",
    "contacts": {
            "$contact": {
                "name": {
                    ".validate": "newData.isString() && newData.val().length < 49 && newData.val().length > 1"
                },
                "businessNumber": {
                    ".validate": "newData.isNumber() && newData.val().length == 9"
                },
                "province": {
                    ".validate": "root.child('province/' + newData.val()).exists()"
                }
            }
        }
  }
}